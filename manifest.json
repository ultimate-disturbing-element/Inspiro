{
    "name": "Inspiro",
    "author": "Abhinav Rai",
    "version": "1.0",
    "description": "Motivational Slogan Everyday",
    "icons": { "128": "motivation.png" },
    
    "browser_action": {
      "default_title": "TodaySpark",
      "default_icon": "motivation.png",
      "default_popup": "index.html"
    },
      
    "permissions": [
       "webRequestBlocking",
       "webRequest",
       "activeTab",
       "tabs",
       "http://*/*",
       "https://*/*"
    ],
    "manifest_version": 2
  }